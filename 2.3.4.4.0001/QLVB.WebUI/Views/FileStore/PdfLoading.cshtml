@model QLVB.DTO.File.PdfViewerModel

@{
    string url = Url.Content("~/ViewerJS/index.html?zoom=page-width#..");    
    string kq = url + Model.filePath;
}

<iframe id="viewerjs" src=@kq width='630' height='470' allowfullscreen webkitallowfullscreen></iframe>

<script type="text/javascript">
    $(document).ready(function () {
        var width = $('#ViewerPdf').width();
        var height = $('#ViewerPdf').height();
        ResizeViewerJs(width - 10, height - 10);
    })
</script>


