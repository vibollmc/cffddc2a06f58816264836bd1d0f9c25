@using Microsoft.Reporting.WebForms;
@using MvcReportViewer;
@model QLVB.DTO.Baocao.ReportLoaivanbandenViewModel
@{
    ViewBag.Title = "Loaivanbanden";
    Layout = "~/Views/Shared/_Layout_null.cshtml";
}

@Html.MvcReportViewerFluent("Report/rptLoaivanbanden.rdlc").ProcessingMode(ProcessingMode.Local).LocalDataSource("DataSetReport", Model.Loaivanban).ReportParameters(new { strngaybd = Model.Thamso.strTungay, strngaykt = Model.Thamso.strDenngay, tensovanban = Model.Thamso.strTenloai, strtendonvi = Model.Thamso.strTendonvi }).Attributes(new { Height = 600, Width = 1000, style = "border: 1px" }).Method(FormMethod.Post)

