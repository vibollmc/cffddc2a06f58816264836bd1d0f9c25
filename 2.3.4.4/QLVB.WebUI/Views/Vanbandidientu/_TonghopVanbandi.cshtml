@model IEnumerable<QLVB.DTO.Vanbandientu.TonghopVBDiViewModel>
@{
    int count = 0;
}

<table class="table table-bordered table-condensed responsive-table" style="width: 100%">
    <tr style="font-weight:bold;">
        <td style="width: 5%; text-align: center;">STT</td>
        <td style="width:auto">Văn bản phát hành</td>
        
        <td style="width: 50%; text-align: center;">Tổng số</td>
        
    </tr>
   @foreach (var m in Model)
   {
       count++;
        <tr id="@m.intid">
            <td style="width:5%; text-align: center;">@count</td>
            <td style="width:auto;">@m.strten</td>

            <td style="width: 50%; text-align: center;cursor:pointer" onclick="OpenDetailTongVB(@m.intid)">@m.intTongVB</td>

        </tr>
   }
    

</table>


<script type="text/javascript">
    $(document).ready(function () {
        HideLoading();
    })
</script>

